@page
@model AjaxAndPost.Pages.StudentsModel
@{
    ViewData["Title"] = "Students";
}

<h1>Students</h1>

<table class="table">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Age</th>
            <th>Republic Credits</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var stu in Model.Students) {
            <tr>
                <td>@stu.fullname</td>
                <td>@stu.age</td>
                <td>@stu.republic_credits</td>
            </tr>
        }
    </tbody>
</table>